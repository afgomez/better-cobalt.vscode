{
	"name": "Better Cobalt",
	"type": "dark",
	"colors": {
		"editor.background": "#071027",
		"editor.foreground": "#ffffff",
		"activityBarBadge.background": "#007acc",
		"sideBarTitle.foreground": "#bbbbbb"
	},
	"tokenColors": [
		{
			"settings": {
				"foreground": "#eeffffff",
				"background": "#263238ff"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#0066cc"
			}
		},
		{
			"name": "String",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#3ad900"
			}
		},
		{
			"name": "Regexp",
			"scope": [
				"string.regexp"
			],
			"settings": {
				"foreground": "#80ffc2"
			}
		},
		{
			"name": "String interpolation",
			"scope": [
				"punctuation.definition.template-expression",
				"punctuation.section.embedded"
			],
			"settings": {
				"foreground": "#9eff80"
			}
		},
		{
			"name": "Interpolation source",
			"scope": [
				"meta.template.expression",
				"source meta.embedded"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Constant",
			"scope": [
				"constant",
				"support.type.object.module",
				"variable.language.this",
				"keyword.control.default"
			],
			"settings": {
				"foreground": "#ff628c"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"keyword.operator"
			],
			"settings": {
				"foreground": "#ff9d00"
			}
		},
		{
			"name": "Function",
			"scope": [
				"meta.definition.function",
				"meta.function.method"
			],
			"settings": {
				"foreground": "#ffdd00"
			}
		},
		{
			"name": "Type",
			"scope": [
				"storage.type"
			],
			"settings": {
				"foreground": "#ffee80"
			}
		},
		{
			"name": "Variable",
			"scope": [
				"variable.scss",
				"variable.sass",
				"variable.other.readwrite.instance.ruby",
				"variable.other.readwrite.class.ruby"
			],
			"settings": {
				"foreground": "#bbbbbb"
			}
		},
		{
			"name": "HTML tag",
			"scope": [
				"meta.tag",
				"entity.name.tag",
				"keyword.operator.assignment meta.tag",
				"punctuation.section.embedded meta.tag"
			],
			"settings": {
				"foreground": "#9effff"
			}
		},
		{
			"name": "CSS Selector Class",
			"scope": [
				"entity.other.attribute-name.class"
			],
			"settings": {
				"foreground": "#5fe460"
			}
		},
		{
			"name": "CSS Selector ID",
			"scope": [
				"entity.other.attribute-name.id.css"
			],
			"settings": {
				"foreground": "#ff9d00"
			}
		},
		{
			"name": "CSS Selector pseudo",
			"scope": [
				"entity.other.attribute-name.pseudo-element"
			],
			"settings": {
				"foreground": "#ffdd00"
			}
		},
		{
			"name": "CSS Selector attribute",
			"scope": [
				"meta.attribute-selector"
			],
			"settings": {
				"foreground": "#ffdd00"
			}
		},
		{
			"name": "CSS property",
			"scope": [
				"support.type.property-name.css"
			],
			"settings": {
				"foreground": "#80ffbb"
			}
		},
		{
			"name": "CSS value",
			"scope": [
				"support.constant.property-value.css"
			],
			"settings": {
				"foreground": "#ffee80"
			}
		}
	]
}
{
	"name": "Better Cobalt",
	"type": "dark",
	"colors": {
		"activityBarBadge.background": "#007acc",
		"editor.background": "#071027",
		"editor.foreground": "#ffffff",
		"editor.lineHighlightBackground": "#00000059",
		"editor.lineHighlightBorder": "#0000",
		"editor.selectionBackground": "#B3653990",
		"editorGutter.background": "#FFFFFF12",
		"editorLineNumber.foreground": "#364350",
		"sideBarTitle.foreground": "#bbbbbb",
		"diffEditor.insertedTextBackground": "#154F006A",
		"diffEditor.removedTextBackground": "#570A0088",
		"terminal.background": "#000000"
	},
	"tokenColors": [
		{
			"settings": {
				"foreground": "#eeffffff",
				"background": "#263238ff"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#0066cc"
			}
		},
		{
			"name": "Support",
			"scope": [
				"support.class",
				"support.variable.dom",
				"support.variable.object"
			],
			"settings": {
				"foreground": "#80FFBB"
			}
		},
		{
			"name": "String",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#3ad900"
			}
		},
		{
			"name": "Regexp",
			"scope": [
				"string.regexp"
			],
			"settings": {
				"foreground": "#80ffc2"
			}
		},
		{
			"name": "String interpolation",
			"scope": [
				"punctuation.definition.template-expression",
				"punctuation.section.embedded"
			],
			"settings": {
				"foreground": "#9eff80"
			}
		},
		{
			"name": "Interpolation source",
			"scope": [
				"meta.template.expression",
				"source meta.embedded"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Constant",
			"scope": [
				"constant",
				"support.type.object.module",
				"variable.language.this",
				"keyword.control.default"
			],
			"settings": {
				"foreground": "#ff628c"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"keyword.operator",
				"source.js storage.type", // function, class, var, const, let.. are all keywords. That way we reserve Type for Typescript
				"source.js storage.modifier"
			],
			"settings": {
				"foreground": "#ff9d00"
			}
		},
		{
			"name": "Entity",
			"scope": [
				"meta.definition.function",
				"meta.definition.method",
				"meta.function.method",
				"entity.name.type"
			],
			"settings": {
				"foreground": "#ffdd00"
			}
		},
		{
			"name": "Entity: Inherited class",
			"scope": [
				"entity.other.inherited-class"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#80FCFF"
			}
		},
		{
			"name": "Type",
			"scope": [
				"storage.type",
				"storage.modifier",
				"support.type",
				"meta.type.annotation",
				"meta.return.type",
				"meta.type entity.name.type",
				"meta.return.type entity.name.type"
			],
			"settings": {
				"foreground": "#ffee80"
			}
		},
		{
			"name": "Variable",
			"scope": [
				"variable.scss",
				"variable.sass",
				"variable.other.readwrite.instance.ruby",
				"variable.other.readwrite.class.ruby"
			],
			"settings": {
				"foreground": "#bbbbbb"
			}
		},
		{
			"name": "HTML tag",
			"scope": [
				"meta.tag",
				"entity.name.tag",
				"meta.tag keyword.operator.assignment",
				"meta.tag support.class"
			],
			"settings": {
				"foreground": "#9effff"
			}
		},
		{
			"name": "JSX tag content",
			"scope": [
				"meta.jsx.children"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "CSS Selector Class",
			"scope": [
				"entity.other.attribute-name.class"
			],
			"settings": {
				"foreground": "#5fe460"
			}
		},
		{
			"name": "CSS Selector ID",
			"scope": [
				"entity.other.attribute-name.id.css"
			],
			"settings": {
				"foreground": "#ff9d00"
			}
		},
		{
			"name": "CSS Selector pseudo",
			"scope": [
				"entity.other.attribute-name.pseudo-element"
			],
			"settings": {
				"foreground": "#ffdd00"
			}
		},
		{
			"name": "CSS Selector attribute",
			"scope": [
				"meta.attribute-selector"
			],
			"settings": {
				"foreground": "#ffdd00"
			}
		},
		{
			"name": "CSS property",
			"scope": [
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#80ffbb"
			}
		},
		{
			"name": "CSS value",
			"scope": [
				"support.constant.property-value.css"
			],
			"settings": {
				"foreground": "#ffee80"
			}
		}
	]
}
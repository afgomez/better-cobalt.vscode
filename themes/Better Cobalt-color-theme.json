{
	"name": "Better Cobalt",
	"type": "dark",
	"colors": {
		"editor.background": "#071027",
		"editor.foreground": "#ffffff",
		"activityBarBadge.background": "#007acc",
		"sideBarTitle.foreground": "#bbbbbb"
	},
	"tokenColors": [
		{
			"settings": {
				"foreground": "#eeffffff",
				"background": "#263238ff"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#0066cc"
			}
		},
		{
			"name": "String",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#3ad900"
			}
		},
		{
			"name": "String interpolation",
			"scope": [
				"punctuation.definition.template-expression",
				"punctuation.section.embedded"
			],
			"settings": {
				"foreground": "#9eff80"
			}
		},
		{
			"name": "Interpolation source",
			"scope": [
				"meta.template.expression",
				"source meta.embedded"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Constant",
			"scope": [
				"constant",
				"support.type.object.module"
			],
			"settings": {
				"foreground": "#ff628c"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"keyword.operator"
			],
			"settings": {
				"foreground": "#ff9d00"
			}
		},
		{
			"name": "Function",
			"scope": [
				"meta.definition.function",
				"meta.function.method"
			],
			"settings": {
				"foreground": "#ffdd00"
			}
		},
		{
			"name": "Type",
			"scope": [
				"storage.type"
			],
			"settings": {
				"foreground": "#ffee80"
			}
		},
		{
			"name": "HTML tag",
			"scope": [
				"meta.tag",
				"keyword.operator.assignment meta.tag",
				"punctuation.section.embedded meta.tag"
			],
			"settings": {
				"foreground": "#9effff"
			}
		}
	]
}